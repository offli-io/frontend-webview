{
  "expo": {
    "name": "Offli",
    "slug": "offli",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/offli-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.app.offli",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow you to take and upload photos for your profile or title picture.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to display nearest activities around you"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/offli-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.ACCESS_FINE_LOCATION"],
      "package": "com.app.offli",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "541ac082-36be-4cfd-8cd1-19200a87a656"
      }
    }
  }
}
